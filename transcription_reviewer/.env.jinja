# AWS Configuration (local development)
AWS_PROFILE_REVIEWER={{ aws.profile_reviewer }}
AWS_REGION={{ aws.region }}

# S3 Configuration
TRANSCRIPTION_BUCKET={{ s3.transcription_bucket }}
TEMPLATE_BUCKET={{ s3.template_bucket }}
AUDIO_BUCKET={{ s3.audio_bucket }}
OUTPUT_BUCKET={{ s3.output_bucket }}
TRANSCRIPTION_PREFIX={{ s3.output_prefix }}

# Bedrock Configuration
BEDROCK_MODEL_ID={{ bedrock.model_id }}
BATCH_MODEL_ID={{ bedrock.batch_model_id }}
BATCH_ROLE_ARN={{ bedrock.batch_role_arn }}

# Batch Processing Configuration
MIN_ENTRIES={{ batch.min_entries }}
MAX_TOKENS={{ batch.max_tokens }}
TEMPERATURE={{ batch.temperature }}

# LLM Backend Selection (AWS_OPUS4.5 | GEMINI2.5)
LLM_BACKEND={{ llm_backend }}

# Google Gemini Configuration (only needed if LLM_BACKEND=GEMINI2.5)
GOOGLE_API_KEY={{ google_api_key }}
GEMINI_MODEL={{ gemini_model }}

# SQS Configuration
SQS_QUEUE_URL={{ sqs.fix_transcribes_queue_url }}
